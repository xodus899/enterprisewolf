<!DOCTYPE html>
<html>
  <head>
    <title>Enterprisewolf</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<div class="container">
  		<ul class= "nav nav-tabs">
	  		<li> <%= link_to "Home", root_path %> </li>
	  		<li> <%= link_to "About us", welcome_about_path %> </li>
	  		<li> <%= link_to "Contact", welcome_contact_path %> </li>
	  		<li> <%= link_to "Features", welcome_features_path %> </li>
	  		<li> <%= link_to "FAQ", welcome_faq_path %> </li>
	  		<li> <%= link_to "Pricing", welcome_pricing_path %> </li>
	  		<li> <%= link_to "Invoices", invoices_path %> </li>
	  		<li> <%= link_to "Login", new_user_session_path %> </li>
	  	</ul>

      <p class="notice"> <%= notice %> </p>
      <p class="alert">  <%=  alert %>  </p>

      <%= yield %>
    </div>
  </body>
</html>
